            "role": "Developer"
        }
    ],
    "homepage": "https://github.com/ngopibareng/rajaongkir-laravel",
    "keywords": [
        "php",
        "laravel",
        "raja ongkir"
    ],
    "require": {
        "php": "^7.3|^8.0",
        "illuminate/support": "~7|~8",
        "guzzlehttp/guzzle": "^7.0.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~5|~6"
    },
    "autoload": {
        "psr-4": {
            "Ngopibareng\\RajaongkirLaravel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ngopibareng\\RajaongkirLaravel\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ngopibareng\\RajaongkirLaravel\\RajaongkirLaravelServiceProvider"
            ],
            "aliases": {
                "RajaongkirLaravel": "Ngopibareng\\RajaongkirLaravel\\Facades\\RajaongkirLaravel"
            }
        }
    }
}
